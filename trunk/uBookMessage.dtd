<?xml version="1.0" encoding="UTF-8"?>
<!ELEMENT uBookMessage (book*, server*)>
<!ATTLIST uBookMessage
    version CDATA   #REQUIRED
    from    CDATA   #REQUIRED
>
<!ELEMENT server EMPTY>
